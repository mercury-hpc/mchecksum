# This gives access to the mchecksum header files
prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=@CMAKE_INSTALL_PREFIX@
libdir=@MCHECKSUM_INSTALL_LIB_DIR@
includedir=@MCHECKSUM_INSTALL_INCLUDE_DIR@

Name: @MCHECKSUM_PACKAGE@
Description: @MCHECKSUM_PACKAGE_DESCRIPTION@
Version: @MCHECKSUM_VERSION_MAJOR@.@MCHECKSUM_VERSION_MINOR@.@MCHECKSUM_VERSION_PATCH@
URL: @MCHECKSUM_PACKAGE_URL@
Requires:
Libs: -L${libdir} @MCHECKSUM_LIBRARIES@
Libs.private: @MCHECKSUM_LIB_DEPENDENCIES@
Cflags: -I${includedir} @MCHECKSUM_INCLUDE_DEPENDENCIES@
